{
  "name": "relative-move",
  "displayName": "relative-move",
  "description": "Move cursor up or down by relative lines after a shortcut-key",
  "version": "0.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/phogtr/relative-move.git"
  },
  "bugs": {
    "url": "https://github.com/phogtr/relative-move/issues"
  },
  "author": "phogtr",
  "engines": {
    "vscode": "^1.90.0"
  },
  "main": "./out/main.js",
  "contributes": {
    "commands": [
      {
        "command": "relative-move.moveInput",
        "title": "Read input and move"
      },
      {
        "command": "relative-move.moveAndSelectInput",
        "title": "Read input, move, and select"
      },
      {
        "command": "relative-move.moveUp",
        "title": "Move up by"
      },
      {
        "command": "relative-move.moveDown",
        "title": "Move down by"
      },
      {
        "command": "relative-move.cancelReadInput",
        "title": "Cancel read input"
      },
      {
        "command": "relative-move.type0",
        "title": "To be able to register the input '0' when activate the extension"
      },
      {
        "command": "relative-move.type1",
        "title": "To be able to register the input '1' when activate the extension"
      },
      {
        "command": "relative-move.type2",
        "title": "To be able to register the input '2' when activate the extension"
      },
      {
        "command": "relative-move.type3",
        "title": "To be able to register the input '3' when activate the extension"
      },
      {
        "command": "relative-move.type4",
        "title": "To be able to register the input '4' when activate the extension"
      },
      {
        "command": "relative-move.type5",
        "title": "To be able to register the input '5' when activate the extension"
      },
      {
        "command": "relative-move.type6",
        "title": "To be able to register the input '6' when activate the extension"
      },
      {
        "command": "relative-move.type7",
        "title": "To be able to register the input '7' when activate the extension"
      },
      {
        "command": "relative-move.type8",
        "title": "To be able to register the input '8' when activate the extension"
      },
      {
        "command": "relative-move.type9",
        "title": "To be able to register the input '9' when activate the extension"
      }
    ],
    "keybindings": [
      {
        "key": "ctrl+r",
        "command": "relative-move.moveInput"
      },
      {
        "key": "ctrl+shift+r",
        "command": "relative-move.moveAndSelectInput"
      },
      {
        "key": "escape",
        "command": "relative-move.cancelReadInput",
        "when": "editorTextFocus && relativeMoveInputRead"
      },
      {
        "key": "ctrl+c",
        "command": "relative-move.cancelReadInput",
        "when": "editorTextFocus && relativeMoveInputRead"
      },
      {
        "key": "w",
        "command": "relative-move.moveUp",
        "when": "editorTextFocus && relativeMoveInputRead"
      },
      {
        "key": "s",
        "command": "relative-move.moveDown",
        "when": "editorTextFocus && relativeMoveInputRead"
      },
      {
        "key": "0",
        "command": "relative-move.type0",
        "when": "editorTextFocus"
      },
      {
        "key": "1",
        "command": "relative-move.type1",
        "when": "editorTextFocus"
      },
      {
        "key": "2",
        "command": "relative-move.type2",
        "when": "editorTextFocus"
      },
      {
        "key": "3",
        "command": "relative-move.type3",
        "when": "editorTextFocus"
      },
      {
        "key": "4",
        "command": "relative-move.type4",
        "when": "editorTextFocus"
      },
      {
        "key": "5",
        "command": "relative-move.type5",
        "when": "editorTextFocus"
      },
      {
        "key": "6",
        "command": "relative-move.type6",
        "when": "editorTextFocus"
      },
      {
        "key": "7",
        "command": "relative-move.type7",
        "when": "editorTextFocus"
      },
      {
        "key": "8",
        "command": "relative-move.type8",
        "when": "editorTextFocus"
      },
      {
        "key": "9",
        "command": "relative-move.type9",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.90.0",
    "@vscode/vsce": "^3.6.2",
    "typescript": "^5.6.0"
  }
}